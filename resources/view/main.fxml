<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<Scene xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.mainController">

         <HBox prefHeight="768.0" prefWidth="1024.0">
      <TabPane prefHeight="200.0" prefWidth="1024.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Belegung">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
          <Tab text="Reservierungen">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                             <!-- Report -->
                             <Button fx:id="createRoom" layoutX="49.0" layoutY="26.0" onMouseClicked="#call_createRoomController" prefHeight="25.0" prefWidth="96.0" text="Create Room" />
                             <!-- Booking -->
                             <Button fx:id="createBooking" layoutX="54.0" layoutY="61.0" onMouseClicked="#call_createBookingController" text="New Booking" />
                             <!-- Update -->
                             <Button fx:id="createGuest" layoutX="55.0" layoutY="99.0" onMouseClicked="#call_createGuestController" text="Create Guest" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
         </HBox>
</Scene>
