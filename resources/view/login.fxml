<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<Scene xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.loginController">
    <AnchorPane prefHeight="200" prefWidth="200">
        <children>
            <Label layoutX="24.0" layoutY="25.0" text="Username" />
            <Label layoutX="26.0" layoutY="83.0" text="Password" />
            <TextField fx:id="username" layoutX="26.0" layoutY="42.0" />
            <TextField fx:id="password" layoutX="26.0" layoutY="100.0" />
            <Button layoutX="70.0" layoutY="145.0" mnemonicParsing="false" onMouseClicked="#login" text="Login" />
         <Label layoutX="10.0" layoutY="6.0" text="Username: user, Password: user" />
        </children></AnchorPane>
</Scene>
