<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Scene xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
    <AnchorPane prefHeight="450.0" prefWidth="600.0">
        <children>
            <Label layoutX="226.0" layoutY="291.0" text="Username" />
            <Label layoutX="226.0" layoutY="349.0" text="Password" />
            <TextField fx:id="username" layoutX="226.0" layoutY="308.0" onAction="#login" />
         <PasswordField fx:id="password" layoutX="226.0" layoutY="366.0" onAction="#login" />
            <Button fx:id="loginButton" layoutX="277.0" layoutY="411.0" mnemonicParsing="false" onMouseClicked="#login" text="Login" />
         <Label layoutX="193.0" layoutY="73.0" text="Hotel Management System">
            <font>
               <Font name="Baskerville Old Face" size="20.0" />
            </font></Label>
         <Label layoutX="74.0" layoutY="14.0" text="NULL POINTER Hotels">
            <font>
               <Font name="Felix Titling" size="40.0" />
            </font>
         </Label>
         <ImageView fitHeight="136.0" fitWidth="137.0" layoutX="232.0" layoutY="130.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../img/login.png" />
            </image>
         </ImageView>
        </children></AnchorPane>
</Scene>
